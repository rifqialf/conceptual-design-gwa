{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.org/schema/infra.json",
    "$defs": {
        "EnschedeBuilding": {
            "$anchor": "EnschedeBuilding",
            "type": {
                "href": "CD-GWA-Profile.json#/$defs/DataContainer"
            },
            "properties": {
                "query": {
                    "type": "string",
                    "default": "SELECT gid, building_type, built_year, ST_AsGeoJSON(ST_FlipCoordinates(geom)) FROM enschede.building_4326 WHERE (building_type = 1 AND built_year >= 2000)"
                },
                "datacontainer": {
                    "type": {
                        "href": "___data container enumeration___"
                    },
                    "default": "PostgreSQL"
                }
            },
            "required": [
                "properties"
            ]
        },
        "EnschedeBusStop": {
            "$anchor": "EnschedeBusStop",
            "type": {
                "href": "CD-GWA-Profile.json#/$defs/DataContainer"
            },
            "query": {
                "query": {
                    "type": "string",
                    "default": "SELECT gid,station_name,ST_AsText(geom), lat,lon FROM enschede.busstop LIMIT 100"
                },
                "datacontainer": {
                    "type": {
                        "href": "___data container enumeration___"
                    },
                    "default": "PostgreSQL"
                }
            },
            "required": [
                "properties"
            ]
        },
        "OpenStreetMap": {
            "$anchor": "OpenStreetMap",
            "type": {
                "href": "___stereotype___"
            },
            "query": {
                "url": {
                    "type": {
                        "href": "___data container enumeration___"
                    },
                    "default": "https://b.tile.openstreetmap.fr/hot/{z}/{x}/{y}.png"
                }
            },
            "required": [
                "properties"
            ]
        },
        "Map": {
            "$anchor": "Map",
            "type": {
                "href": "___UI Element stereotype___"
            },
            "properties": {
                "data": {
                    "type": "array",
                    "items": {
                        "type": {
                            "href": "___data container stereotypes___"
                        },
                        "default": [
                            {
                                "href": "___Enschede Buildilng___"
                            },
                            {
                                "href": "___Enschede Busstop___"
                            }
                        ]
                    },
                    "default": "https://b.tile.openstreetmap.fr/hot/{z}/{x}/{y}.png"
                }
            },
            "required": [
                "properties"
            ],
            "methods": {
                "updateMap": {
                    "return": null
                },
                "dragMap": {
                    "return": null
                },
                "zoomMap": {
                    "return": null
                },
                "selectDisplayedData": {
                    "return": null
                },
                "showDataInformation": {
                    "return": null
                }
            }

        },
    }
}